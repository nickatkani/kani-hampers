{
  "name": "kani-hampers",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "proxy": "http://localhost:5000",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "seed": "npx tsx src/seedFirebase.ts",
    "seed:products": "cd server && node seed.js",
    "seed:config": "cd server && node seed-config.js",
    "seed:all": "cd server && node seed-all.js",
    "add-test-order": "npx tsx src/addTestOrder.ts",
    "server": "cd server && npm run dev",
    "start:full": "concurrently \"npm run server\" \"npm run dev\""
  },
  "dependencies": {
    "firebase": "^11.10.0",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.263.1",
    "razorpay": "^2.9.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/jspdf": "^1.3.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
